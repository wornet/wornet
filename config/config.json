{

    "express": {
        "view engine": "jade"
    },

    "middleware": {

        "devtools": {
            "enabled": true,
            "priority": 10,
            "module": {
                "name": "kraken-devtools",
                "arguments": [
                    "path:./public",
                    "path:./.build",
                    {   
                        "css": {
                            "module": "kraken-devtools/plugins/stylus",
                            "files": "/css/**/*.css"
                        },
                        "copier": {
                            "module": "kraken-devtools/plugins/copier",
                            "files": "**/*"
                        }
                    }
                ]
            }
        },
        
        "expressView": {
            "priority": 100,
            "enabled": true,
            "module": {
                "name": "engine-munger/lib/expressView"
            }
        },
        

        "fileNotFound": {
            "enabled": true,
            "priority": 130,
            "module": {
                "name": "kraken-js/middleware/404",
                "arguments": [ "errors/404" ]
            }
        },

        "router": {
            "module": {
                "arguments": [{ "directory": "path:./controllers/" }]
            }
        },

        "serverError": {
            "enabled": true,
            "priority": 140,
            "module": {
                "name": "kraken-js/middleware/500",
                "arguments": [ "errors/500" ]
            }
        },

        "session": {
            "enabled": true,
            "priority": 100,
            "module": {
                "name": "express-session",
                "arguments": [
                    {
                        "key": "w",
                        "secret": "6qed36sQyAurbQCLNE3X6r6bbtSuDEcU",
                        "cookie": {
                            "path": "/",
                            "httpOnly": true,
                            "maxAge": null
                        },
                        "resave": true,
                        "saveUninitialized": true,
                        "proxy": null
                    }
                ]
            }
        },

        "static": {
            "module": {
                "arguments": [ "path:./.build" ]
            }
        }

    },

    "view engines": {

        "dust": {
            "module": "adaro",
            "renderer": {
                "method": "dust",
                "arguments": [{ "cache": false }]
            }
        },

        "html": {
            "name": "ejs",
            "module": "ejs",
            "renderer": "renderFile"
        },

        "jade": {
            "module": "consolidate"
        },

        "js": {
            "module": "adaro",
            "renderer": {
                "method": "js",
                "arguments": [{ "cache": false }]
            }
        }

    },

